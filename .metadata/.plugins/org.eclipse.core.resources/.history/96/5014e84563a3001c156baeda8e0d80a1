package Exe0314;

public class If01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=0, b =15;
		if (a>10) 
		{
			System.out.println("a="+a);
		}
		System.out.println("----------");
		if (b>10) 
		{
			System.out.println("b="+b);
		}
		else
		{
			System.out.println(a);
		}
	}

}
